{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "my-command",
      "identifier": "collapse-groups",
      "script": "my-command.js",
      "handlers": {
        "actions": {
          "CollapseAllGroups.finish": "onCollapse"
        }
      }
    }
  ],
  "version": "0.1.0",
  "description": "Collapses all groups in the layer list, except your current selection, this overrides the default sketch behvaiour. Ensure 'Automatically Expand Groups' is enabled.",
  "name": "better-collapse-groups",
  "identifier": "better-collapse-groups",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml",
  "author": "Luke Finch",
  "authorEmail": "lukechristianfinch@gmail.com"
}